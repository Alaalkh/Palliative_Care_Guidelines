<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Doctors.DoctorProfile">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/textView"
            android:layout_width="431dp"
            android:layout_height="269dp"

            android:background="@drawable/profile_card"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="match_parent"
            android:layout_height="441dp"
            android:layout_marginStart="30dp"
            android:layout_marginEnd="30dp"
            android:layout_marginBottom="72dp"
            android:background="@drawable/textround"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/emailprofile1"
            app:layout_constraintVertical_bias="0.439"
            tools:ignore="MissingClass,MissingConstraints" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="24dp"
            android:src="@drawable/userpic"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="match_parent"
            android:layout_height="201dp"
            android:scaleType="centerCrop"
            android:src="@drawable/circles"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:textDirection="rtl"
            android:id="@+id/profilename1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="الاسم"
            android:textColor="@color/white"
            android:textSize="22sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView3" />

        <TextView
            android:id="@+id/emailprofile1"
            android:layout_width="wrap_content"
            android:textDirection="rtl"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="الايميل"
            android:textColor="@color/white"
            android:textSize="20sp"
            android:textStyle="normal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/profilename1" />

        <TextView
            android:id="@+id/txt"
            android:layout_width="wrap_content"
            android:layout_height="47dp"
            android:layout_marginTop="40dp"
            android:layout_marginEnd="20dp"
            android:text="رقم الهاتف:"
            android:textColor="@color/blue"
            android:textDirection="rtl"
            android:textSize="23sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/textView4"
            app:layout_constraintTop_toTopOf="@+id/textView4" />

        <TextView
            android:id="@+id/txt2"
            android:layout_width="wrap_content"
            android:layout_height="47dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="20dp"
            android:text="تاريخ الميلاد:"
            android:textColor="@color/blue"
            android:textDirection="rtl"
            android:textSize="23sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/textView4"
            app:layout_constraintTop_toBottomOf="@+id/textView14" />

        <TextView
            android:id="@+id/textView14"
            android:layout_width="wrap_content"
            android:layout_height="47dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="20dp"
            android:text="العنوان:"
            android:textColor="@color/blue"
            android:textDirection="rtl"
            android:textSize="23sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/textView4"
            app:layout_constraintTop_toBottomOf="@+id/txt" />

        <TextView
            android:id="@+id/txt3"
            android:layout_width="wrap_content"
            android:layout_height="47dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="20dp"
            android:text="كلمة المرور:"
            android:textColor="@color/blue"
            android:textDirection="rtl"
            android:textSize="23sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/textView4"
            app:layout_constraintTop_toBottomOf="@+id/txt2" />

        <Button
            android:id="@+id/logout1"
            android:layout_width="283dp"
            android:layout_height="59dp"
            android:layout_marginEnd="55dp"
            android:layout_marginBottom="56dp"
            android:backgroundTint="@color/blue"
            android:text="تسجيل خروج!"
            android:textSize="22sp"
            android:textDirection="rtl"
            android:textColor="@color/white"
            app:layout_constraintBottom_toBottomOf="@+id/textView4"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/phoneprofile1"
            android:layout_width="163dp"
            android:layout_height="44dp"
            android:layout_marginTop="40dp"
            android:layout_marginEnd="48dp"
            android:text="TextView"
            android:textSize="16sp"
            android:textStyle="bold"
            android:textDirection="rtl"
            app:layout_constraintEnd_toStartOf="@+id/txt"
            app:layout_constraintTop_toTopOf="@+id/textView4" />

        <TextView
            android:id="@+id/addressprofile1"
            android:layout_width="161dp"
            android:layout_height="44dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="80dp"
            android:text="TextView"
            android:textSize="16sp"
            android:textDirection="rtl"
            android:textStyle="bold"
            app:layout_constraintEnd_toStartOf="@+id/textView14"
            app:layout_constraintTop_toBottomOf="@+id/phoneprofile1" />

        <TextView
            android:id="@+id/birth_dateprofile1"
            android:layout_width="158dp"
            android:layout_height="44dp"
            android:layout_marginTop="24dp"
            android:layout_marginEnd="32dp"
            android:text="TextView"
            android:textDirection="rtl"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toStartOf="@+id/txt2"
            app:layout_constraintTop_toBottomOf="@+id/addressprofile1" />

        <TextView
            android:id="@+id/password_profile1"
            android:layout_width="167dp"
            android:layout_height="44dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="40dp"
            android:text="TextView"
            android:textSize="16sp"
            android:textStyle="bold"
            android:textDirection="rtl"
            app:layout_constraintEnd_toStartOf="@+id/txt3"
            app:layout_constraintTop_toBottomOf="@+id/birth_dateprofile1" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>